#!/usr/bin/python3
"""
Module that factorize numbers
"""
from sys import argv


def factorize(value):
    if value < 2:
        return

    i = 2
    while value % i:
        i += 1

    print("{:.0f}={:.0f}*{:.0f}".format(value, value / i, i))


if __name__ == "__main__":
    if len(sys.argv) != 2:
        sys.exit()

    try:
        with open(sys.argv[1]) as file:
            line = file.readline()

            while line:
                value = int(line.strip())
                factorize(value)
                line = file.readline()
    except Exception as e:
        pass
